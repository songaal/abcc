<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<search-page>
    <css></css>
    <javascript></javascript>
    <realtime-popular-keyword-url></realtime-popular-keyword-url>
    <relate-keyword-url></relate-keyword-url>
    <search-category-list>
        <search-category order="1" name="FILM" id="film">
            <body-field>&lt;div&gt;$description&lt;/div&gt;
&lt;div&gt;$price | $rating&lt;/div&gt;
&lt;div&gt;Actors: $actors&lt;/div&gt;</body-field>
            <search-query>cn=film&amp;fl=fid,title,description:150,category,price,length,rating,actors&amp;se={total_index:#keyword:10:16}&amp;ht=&lt;b&gt;:&lt;/b&gt;&amp;ud=keyword:#keyword</search-query>
            <title-field>&lt;a href="#$fid"&gt;$title&lt;/a&gt;</title-field>
        </search-category>
    </search-category-list>
    <search-list-size>10</search-list-size>
    <total-search-list-size>5</total-search-list-size>
</search-page>
